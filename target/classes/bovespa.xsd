<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="ComportamentoPapeis">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Papel" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute type="xs:string" name="Codigo" use="optional"/>
                <xs:attribute type="xs:string" name="Nome" use="optional"/>
                <xs:attribute type="xs:string" name="Ibovespa" use="optional"/>
                <xs:attribute type="xs:string" name="Data" use="optional"/>
                <xs:attribute type="xs:string" name="Abertura" use="optional"/>
                <xs:attribute type="xs:string" name="Minimo" use="optional"/>
                <xs:attribute type="xs:string" name="Maximo" use="optional"/>
                <xs:attribute type="xs:string" name="Medio" use="optional"/>
                <xs:attribute type="xs:string" name="Ultimo" use="optional"/>
                <xs:attribute type="xs:string" name="Oscilacao" use="optional"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>